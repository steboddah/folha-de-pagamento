using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Folha_de_Pagamento.Classe
{
    class Normal : Funcionarios
    {
        
        public Normal() { }// construtor
        public Normal(string nome, int horas, double thoras)
        {
            Nome = nome;
            Horas = horas;
            THoras = thoras;
        }
     
        public override double Salario()   //calculo dos funcionarios normais
        {
            return Horas * THoras;
        }
    }
}
